Env.Var.ddns_service.Header = Service
Env.Var.ddns_service.Value.dyndns = dyndns.org

Env.Var.ddns_username.Header = Username
Env.Var.ddns_username.Error.value = The user name must be composed of 1 to 25 alphanumeric characters or minus sign '-', and the first character must be a letter.

Env.Var.ddns_password.Header = Password
Env.Var.ddns_password.Error.value = The password must be at least 5 characters long.

Env.Var.ddns_hostname.Header = Domain name
Env.Var.ddns_hostname.Error.value = The host name should consist of 1 to 20 alphanumeric characters or minus sign '-'.

